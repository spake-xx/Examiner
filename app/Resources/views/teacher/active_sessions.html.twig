<table class="table table-stripped table-hover">
    <tr>
        <th>Quiz</th>
        <th>Start</th>
        <th>End</th>
        <th>Left</th>
        <th>Pupilz</th>
    </tr>
    {% for session in sessions %}
    <tr>
        <td>{{ session.quiz.name }}</td>
        <td>{{ session.started|date('Y-m-d H:i:s') }}</td>
        <td>{{ session.end|date('Y-m-d H:i:s') }}</td>
        <td>left</td>
        <td><a ><i class="fa fa-binoculars"></i></a></td>
    </tr>
    {% endfor %}
</table>