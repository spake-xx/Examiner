{% extends 'teacher_base.html.twig' %}
{% block heading %}
    All sesje
{% endblock %}
{% block content %}
    <table class="table table-stripped table-hover">
        <tr>
            <th>Quiz</th>
            <th>Start</th>
            <th>End</th>
            <th>Pupilz</th>
        </tr>
        {% for session in sessions %}
            <tr>
                <td>{{ session.quiz.name }}</td>
                <td>{{ session.started|date('Y-m-d H:i:s') }}</td>
                <td>{{ session.end|date('Y-m-d H:i:s') }}</td>
                <td><a ><i class="fa fa-binoculars"></i></a></td>
            </tr>
        {% endfor %}
    </table>
    <img src="http://www.wykop.pl/cdn/c3201142/comment_mnMwz7BcpZ1nK8wZBrvwVCzv7rJJ9Ap8,w400.jpg"/>
{% endblock %}
